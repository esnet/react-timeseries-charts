{
    "name": "react-timeseries-charts",
    "version": "1.0.0",
    "description": "Declarative timeseries charts",
    "keywords": [
        "d3",
        "charts",
        "react",
        "timeseries"
    ],
    "homepage": "http://software.es.net/react-timeseries-charts",
    "author": "ESnet Tools Team <tools@es.net>",
    "repository": "esnet/react-timeseries-charts",
    "main": "dist/entry.js",
    "typings": "dist/entry.d.ts",
    "files": [
        "dist"
    ],
    "bugs": {
        "url": "https://github.com/esnet/react-timeseries-charts/issues"
    },
    "scripts": {
        "docs": "typedoc --json ../website_old/src/doc.json --mode modules ./src/*.tsx --ignoreCompilerErrors",
        "lint": "eslint src/components/*.js",
        "test": "npm run lint",
        "clean": "rimraf dist",
        "build": "tsc -p tsconfig.json",
        "rollup": "tsc -p tsconfig.json  && cross-env NODE_ENV=production rollup -c && cross-env NODE_ENV=development rollup -c",
        "start-website": "react-scripts start",
        "build-website": "echo \"*** Building website\n\" && rm -rf docs && react-scripts build && mv build docs",
        "precommit": "lint-staged",
        "prettier": "prettier --print-width 100 --tab-width 4 --write \"./src/**/*.ts*\""
    },
    "lint-staged": {
        "*.js": [
            "prettier --print-width 100 --tab-width 4 --write",
            "git add"
        ]
    },
    "pre-commit": [
        "lint",
        "build"
    ],
    "license": "BSD-3-Clause-LBNL",
    "dependencies": {
        "@types/chroma-js": "^1.3.3",
        "@types/d3-ease": "^1.0.7",
        "@types/d3-format": "^1.2.1",
        "@types/d3-scale": "^1.0.11",
        "@types/d3-shape": "^1.3.1",
        "@types/d3-time-format": "^2.1.0",
        "@types/invariant": "^2.2.29",
        "@types/lodash": "^4.14.100",
        "@types/lodash.isobject": "^3.0.3",
        "@types/moment-duration-format": "^1.3.8",
        "@types/react": "^16.0.36",
        "array.prototype.fill": "^1.0.1",
        "babel-runtime": "^6.23.0",
        "chroma-js": "^1.3.6",
        "colorbrewer": "^1.1.0",
        "d3-ease": "^1.0.3",
        "d3-format": "^1.2.0",
        "d3-interpolate": "^1.1.5",
        "d3-scale": "^1.0.6",
        "d3-scale-chromatic": "^1.1.1",
        "d3-selection": "^1.1.0",
        "d3-shape": "^1.2.0",
        "d3-time": "^1.0.7",
        "d3-time-format": "^2.0.5",
        "d3-transition": "^1.1.0",
        "flexbox-react": "git+https://github.com/esnet/flexbox-react.git",
        "immutable": "^4.0.0-rc.12",
        "invariant": "^2.1.1",
        "lodash": "^4.17.4",
        "lodash.isobject": "^3.0.2",
        "merge": "^1.2.0",
        "moment": "^2.18.1",
        "moment-duration-format": "^2.2.1",
        "react-hot-loader": "^4.2.0"
    },
    "devDependencies": {
        "@types/colorbrewer": "^1.0.29",
        "cross-env": "^5.1.3",
        "pondjs": "1.0.0-alpha.4",
        "react": "^16.2.0",
        "react-axis": "1.0.0",
        "react-dom": "^16.2.0",
        "rimraf": "^2.6.2",
        "rollup": "^0.55.3",
        "rollup-plugin-commonjs": "^8.3.0",
        "rollup-plugin-css-only": "^0.4.0",
        "rollup-plugin-filesize": "^1.5.0",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-node-resolve": "^3.0.2",
        "rollup-plugin-postcss": "^1.2.8",
        "rollup-plugin-replace": "^2.0.0",
        "rollup-plugin-sizes": "^0.4.2",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-typescript2": "^0.11.0",
        "rollup-plugin-uglify": "^3.0.0",
        "typedoc": "^0.11.1",
        "typescript": "^3.4.5"
    },
    "peerDependencies": {
        "pondjs": "1.0.0-alpha.4",
        "react": "16.2.0",
        "react-dom": "16.2.0"
    }
}
