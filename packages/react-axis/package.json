{
    "name": "react-axis",
    "version": "1.0.0",
    "description": "An axis component built with React",
    "keywords": ["react", "axis", "timezones", "timezone", "time", "moment", "d3", "charts"],
    "homepage": "http://software.es.net/react-axis",
    "author": "ESnet Software Engineering Group <tools@es.net>",
    "repository": "esnet/react-timeseries-charts",
    "main": "dist/entry.js",
    "typings": "dist/entry.d.ts",
    "files": ["dist"],
    "bugs": {
        "url": "https://github.com/esnet/react-timeseries-charts/issues"
    },
    "scripts": {
        "docs":
            "echo \"*** Building API docs\n\" && react-docgen src/components -x js -o ../website/src/api/docs.json --pretty",
        "lint": "eslint src/components/*.js",
        "test": "npm run lint",
        "clean": "rimraf dist",
        "build": "tsc -p tsconfig.json",
        "precommit": "lint-staged",
        "prettier": "prettier --print-width 100 --tab-width 4 --write \"./src/**/*.ts*\""
    },
    "lint-staged": {
        "*.js": ["prettier --print-width 100 --tab-width 4 --write", "git add"]
    },
    "pre-commit": ["lint", "build"],
    "license": "BSD-3-Clause-LBNL",
    "dependencies": {
        "@types/d3-format": "^1.2.1",
        "@types/d3-scale": "^1.0.11",
        "@types/react": "^16.0.36",
        "d3-format": "^0.5.1",
        "d3-scale": "^0.7.2",
        "lodash": "^4.17.5",
        "merge": "^1.2.0",
        "moment": "^2.15.2",
        "moment-timezone": "^0.5.10",
        "react-transition-group": "^2.2.1"
    },
    "peerDependencies": {
        "react": "16.2.0",
        "react-dom": "16.2.0"
    },
    "devDependencies": {
        "typescript": "2.7.1",
        "tslib": "^1.9.0"
    }
}
